<% provide(:title, 'Search For a Mentor') %>
<h2>Search For a Mentor</h2>
<div class="row">
  <% @mentors.each do |mentor| %>
      <div class="col-xs-4 col-sm-3 col-md-2">
        <%= link_to((image_tag "#{mentor.photo_url}", alt: "{mentor.last_name}", title: "#{mentor.first_name + " " + mentor.last}"), "/mentor/#{mentor.id}", class: "thumbnail") %>
      </div>
  <% end %>
</div>

